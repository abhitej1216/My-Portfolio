
.navbar {
    background-color: darkblue;
    border-radius: 24px;
}

.navbar ul {
    overflow: auto;
}

.navbar li {
    float: left;
    list-style: none;
    margin: 13px 20px;
}

.navbar li a:hover {
    background-color: red;
    border-radius: 6px;
}

.navbar li a {
    padding: 3px 3px;
    text-decoration: none;
    color: white;
}

.navbar input {
    border: 2px darkslateblue;
    border-radius: 14px;
    padding: 3px 4px;
    width: 350px;
}
